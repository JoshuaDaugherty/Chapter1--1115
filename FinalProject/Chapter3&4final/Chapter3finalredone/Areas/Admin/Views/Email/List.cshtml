@model List<User>
@{
    ViewBag.Title = "Admin Email List";
}
<h1 class="text-white">Admin Email List</h1>

<a class="btn btn-primary" asp-area="Admin" asp-controller="Email" asp-action="Add">Add Contact</a>
<table class="table">
    <tr class="text-warning">
        

        <th>
            Email
        </th>

       
        <th></th>
    </tr>

    @foreach (var user in Model)
    {
        <tr class="text-warning">
            
            <td>@user.Email</td>
           



            <td>

                <a asp-controller="Email" asp-action="Add" asp-route-id=@user.UserId >Edit Email</a>
                <a asp-controller="Email" asp-action="Delete" asp-route-id=@user.UserId >Delete Email</a>


            </td>
        </tr>
    }
</table>
